n.sims<-10000 # setting number of simulations
ci.ratio <- function (p1, p2, p1se, p2se, ...)
{
p.1<-rnorm(n.sims,p1,p1se)
p.2<-rnorm(n.sims,p2,p2se)
ratiop1p2<-p.1/p.2
int.95<-quantile(ratiop1p2,c(.025,.975))
return(int.95)
}
